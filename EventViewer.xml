							<OnChar>
								self.EventID, self.EventTemplate = GroupCalendar.AutoCompleteEventTitle(self)
								self.GotChar = true
							</OnChar>
							<OnTextChanged>
								if not self.GotChar then
									self.EventTemplate = nil
								end
								self.GotChar = false
							</OnTextChanged>
							<OnEditFocusGained>
								self.GotChar = false
								self:HighlightText()
							</OnEditFocusGained>
							<OnEditFocusLost>
								if GroupCalendar.EventViewer.IsNewEvent and (self.EventID or self.EventTemplate) then
									if self.EventTemplate then
										GroupCalendar.EventViewer:SetEventType(self.EventTemplate.mType)
										GroupCalendar.EventViewer:LoadEventDefaults(self.EventTemplate)
									else
										GroupCalendar.EventViewer:SetEventType(self.EventID)
										GroupCalendar.EventViewer:LoadEventDefaults()
									end
								end
								self:HighlightText(0, 0)
							</OnEditFocusLost>
							<OnEscapePressed>
								self:ClearFocus()
							</OnEscapePressed>
							<OnEnterPressed>
								GroupCalendar.EventViewer:DoneViewing()
							</OnEnterPressed>
